   "source": [
    "n = int(input(\"Digite um número inteiro: \"))\n",
    "cont = 0\n",
    "i = 0\n",
    "\n",
    "while i <= n or cont < 2:\n",
    "    i = i + 1\n",
    "    x = n % i\n",
    "    if x == 0:\n",
    "        cont = cont + 1\n",
    "\n",
    "if cont <= 2:\n",
    "    print(\"primo\")\n",
    "else:\n",
    "\n",
    "    print(\"não primo\")\n",
